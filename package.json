{
  "name": "rack-designer-vue",
  "version": "1.0.0",
  "description": "Vue.js Rack Layout Designer Component with PrimeVue",
  "type": "module",
  "main": "./lib/rack-designer.umd.js",
  "module": "./lib/rack-designer.es.js",
  "exports": {
    ".": {
      "import": "./lib/rack-designer.es.js",
      "require": "./lib/rack-designer.umd.js"
    },
    "./style.css": "./lib/rack-designer.css"
  },
  "files": [
    "lib",
    "RackDesigner.vue",
    "src",
    "data",
    "composables",
    "api",
    "types.ts",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --config vite.config.lib.js",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "vue": "^3.4.0",
    "primevue": "^4.5.0",
    "@primevue/themes": "^4.5.0",
    "primeicons": "^7.0.0"
  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "@primevue/themes": "^4.5.4",
    "@vitejs/plugin-vue": "^6.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.4",
    "vite": "^7.3.0",
    "vitest": "^2.1.9",
    "vue": "^3.4.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "component",
    "rack",
    "designer",
    "server",
    "chassis",
